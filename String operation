#include <stdio.h>
#include <string.h>

int main() {
    char name[20];
    
    printf("Enter the name");
    scanf("%s", &name);
    printf("%s\n", name[20]);
    int length = strlen(name);
    printf("%d\n",length);
    strcpy(name, "anu");
    printf("%s\n", name );
    int cmp=strcmp("bo","boo");
    printf("%d\n",cmp);
    strcat(name,"coder");
    printf("%s\n",name);
}
