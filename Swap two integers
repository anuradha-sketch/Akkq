#include <stdio.h>
int main() {
    int a,b;
    printf("Hi anu!\n");
    printf("Enter the value of a and b:");
    scanf("%d%d",&a,&b);
    a=a+b;
    b=b-a;
    a=a-b;
    printf("The swapping result:\n");
    printf("a is:%d \nb is:%d\n",a,b);
    
 return 0;
}
