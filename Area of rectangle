#include <stdio.h>
int main() {
    int len,wid,area;
    printf("Hi anu!\n");
    printf("enter the length and width of rectangle:");
    scanf("%d%d%d", &len,&wid);
    area=len*wid;
    printf("Area of tectangle is:%d",area);

    return 0;
}
