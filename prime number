#include <stdio.h>
int main(){
    int i,num,isprime =1;
    printf("Enter the number");
    scanf("%d",&num);
    for(i=2;i<=num/2;i++){
        if (num%2==0){
            printf("its not a prime\n");
        }
        else{
            printf("its a prime\n");
        }
    }

    return 0;
}
