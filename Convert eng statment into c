#include <stdio.h>
int main() {
    int age;
    char name[20];
    printf("Hi anu!\n");
    printf("Enter your name:");
    scanf("%s",&name);
    printf("Enter your age:");
    scanf("%d",&age);
    printf("Hello %s!, you are %d years old.",name,age);
    
 return 0;
}
